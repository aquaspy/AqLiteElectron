{
  "name": "AquaStar",
  "version": "0.6.0",
  "description": "A custom aqlite launcher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist-w": "electron-builder -w",
    "dist-l": "electron-builder -l",
    "dist-wl": "electron-builder -wl",
    "dist-m": "electron-builder -m"
  },
  "keywords": [
    "aqw;",
    "aquastar;",
    "aqlite;"
  ],
  "author": "aquastar",
  "license": "MIT",
  "devDependencies": {
    "electron": "^4.2.12",
    "electron-builder": "^22.8.1",
    "nw-flash-trust": "^0.3.0"
  },
  "build": {
    "asar": false,
    "appId": "aquastar",
    "mac": {
      "category": "Games"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Game"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "icon": "Icon/Icon.png"
  },
  "dependencies": {
    "nw-flash-trust": "^0.3.0"
  }
}
